#include <iostream>
#include <iomanip>

using namespace std;

int userfeed(int x){
    
    int Car;
    int Hours;
    double Charge=12.00;
    double extra_Charge;
    double max_Charge;
    for(int i=0;i<x;i++){
        
        while(1){
        
            cout<<"Enter The Car no:\n Enter hours in that order\n:";
            cin>>Car>>Hours; cin.ignore();
            
            if(Hours>3){
                
                double extra_Charge = Charge+(0.90*Hours);
                cin>>extra_Charge; cin.ignore();
                
            }else if(Hours>25){
                
                double max_Charge=20;
                cin>>max_Charge; cin.ignore();
            
                
            }else{
                    cin>>Charge; cin.ignore();
            };
        };
         
        cout<<setw(3)<<"Cars:"<<setw(10)<<"Hours:"<<setw(20)<<"Rate per hour:"<<"\n";
        cout<<setw(3)<<1<<setw(10)<<20<<setw(20)<<10.3<<"\n";
        cout<<setw(3)<<2<<setw(10)<<9<<setw(20)<<11.3<<"\n";
        cout<<setw(3)<<3<<setw(10)<<10<<setw(20)<<20<<"\n";
        break;    
    };
    
    return 0;
}
int main()
{
    userfeed(3);
    return 0;
}

